#Arithmetic Operators
def arith(x,y):
    print(x+y)
    print(x-y)
    print(x*y)
    print(x/y)
    print(x//y)
    print(x%y)
    
a=int(input())
b=int(input())
arith(a,b)

#Sqaure Root
def sqrt(x):
    print(x**2)

a=int(input())
sqrt(a)

#Cube Root
def cubt(x):
    print(x**3)
    
a=int(input())
cubt(a)
